<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.findmatch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Adapter.BoardAdapter"
            android:label="@string/title_activity_board_adapter"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Activity.BoardActivity" />
        <activity android:name=".Activity.SigninActivity"></activity>
        <activity android:name=".Activity.MakeTeamActivity" />
        <activity android:name=".Activity.MypageActivity"/>

        <activity
            android:name=".Activity.ResultActivity"
            android:parentActivityName=".Activity.MainActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.TeamManageActivity" />
        <activity android:name=".Activity.LoginActivity" />
    </application>

</manifest>